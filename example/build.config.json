{
  "defaultLanguage": "de",
  "languages": [
    {
      "id": "de",
      "dataPath": "data/locales"
    },
    "en"
  ],
  "pages": [
    {
      "id": "index",
      "path": "/",
      "templateFile": "src/pages/index.js",
      "translationKey": "index"
